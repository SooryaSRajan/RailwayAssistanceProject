<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.ssr_projects.railasistant">

    <application
        android:allowBackup="false"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:testOnly="false"
        android:theme="@style/AppTheme">
        <activity android:name=".Registration.ReservationTicketActivity" />
        <activity android:name=".Registration.ReservationActivity">
        </activity>
        <activity android:name=".Registration.QRCodeActivity" />
        <activity android:name=".Registration.ReservationLogin" />
        <activity android:name=".VoiceResultActivity" android:screenOrientation="portrait" />
        <activity
            android:name=".MainActivity"
            android:screenOrientation="portrait">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".PassCodeActivity" android:screenOrientation="portrait" />
    </application>

    <uses-permission android:name="android.permission.RECORD_AUDIO" />

</manifest>